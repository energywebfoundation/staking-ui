    "@sentry/angular": "^6.2.0",
    "@types/disposable-email-domains": "^1.0.2",
    "core-js": "2.5.4",
    "disposable-email-domains": "^1.0.59",
    "iam-client-lib": "6.0.0-alpha.21",
    "ngx-spinner": "^11.0.2",
    "ngx-toastr": "13.2.1",
    "rxjs": "6.6.7",
