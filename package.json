{
  "name": "staking-pool-ui",
  "version": "1.0.0",
  "description": "Decentralized Identity and Access Management Switchboard",
  "author": "EWF",
  "license": "GPL-3.0-or-later",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "ng build --watch",
    "build-volta": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --configuration production --aot -c=staging",
    "build-prod": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --configuration production --aot -c=production",
    "build-no-kyc": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --configuration production --aot -c=dev",
    "build": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --configuration production --aot -c=dev",
    "analyze": "npm run build -- --statsJson=true && webpack-bundle-analyzer dist/stats.json",
    "postinstall": "node patch.js && patch-package",
    "test": "ng test --code-coverage",
    "test:headless": "ng test --karma-config=karma-headless.conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
